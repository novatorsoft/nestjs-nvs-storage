{
  "name": "nestjs-nvs-storage",
  "version": "2.5.1",
  "description": "",
  "author": "Ismet Kizgin",
  "private": false,
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/libs/nvs-storage/index.js",
  "type": "module",
  "files": [
    "dist",
    "packages.json"
  ],
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start nvs-storage-service",
    "start:dev": "nest start --watch nvs-storage-service",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/nvs-storage-service/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/nvs-storage-service/test/jest-e2e.json",
    "release": "release-it"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@aws-sdk/client-s3": "^3.665.0",
    "@aws-sdk/s3-request-presigner": "^3.665.0",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.0",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "file-type": "^21.1.1",
    "graphql": "^16.9.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@release-it/conventional-changelog": "^8.0.2",
    "@types/express": "^4.17.17",
    "@types/file-type": "^10.9.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "aws-sdk-client-mock": "^4.0.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "mockingbird": "^2.1.2",
    "prettier": "^3.0.0",
    "release-it": "^17.6.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@lib/nvs-storage(|/.*)$": "<rootDir>/libs/nvs-storage/src/$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(file-type)/)"
    ]
  },
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72",
  "release-it": {
    "git": {
      "requireBranch": "master",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "conventionalcommits"
        },
        "infile": "CHANGELOG.md"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ismetkizgin/nestjs-nvs-storage"
  },
  "keywords": [
    "nestjs",
    "plugin",
    "nestjs-plugin",
    "nvs-storage",
    "storage",
    "s3",
    "aws-s3"
  ],
  "bugs": {
    "url": "https://github.com/ismetkizgin/nestjs-nvs-storage/issues-module"
  },
  "homepage": "https://github.com/ismetkizgin/nestjs-nvs-storage#readme-module"
}
